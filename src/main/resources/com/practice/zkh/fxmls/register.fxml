            <Font name="GOST type A" size="15.0" />
         </font>
      </TextField>
      <ComboBox fx:id="districtCB" layoutX="246.0" layoutY="14.0" prefHeight="25.0" prefWidth="181.0" promptText="Выберите район" />
      <ComboBox fx:id="streetCB" layoutX="14.0" layoutY="49.0" prefHeight="25.0" prefWidth="181.0" promptText="Выберите улицу" />
      <ComboBox fx:id="houseCB" layoutX="246.0" layoutY="49.0" prefHeight="25.0" prefWidth="181.0" promptText="Выберите номер дома" />
