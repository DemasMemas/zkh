            <Font name="GOST type A" size="15.0" />
         </font>
      </TextField>
      <Label fx:id="messageLabel" layoutX="60.0" layoutY="122.0" text="Если у вас нет счётчика, пишите 0 в качестве значения" textAlignment="CENTER">
